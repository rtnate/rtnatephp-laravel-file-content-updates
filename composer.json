{
    "name": "rtnatephp/laravel-file-content-updates",
    "description": "Update Laravel CMS models via Markdown files with frontmatter",
    "type": "library",
    "autoload": {
        "psr-4": {
            "RTNatePHP\\LaravelFileContentUpdates\\": "src/"
        }
    },
    "authors": [
        {
            "name": "nate",
            "email": "nate@rtelectronix.com"
        }
    ],
    "require": {
        "laravel/framework": "^8.0",
        "spatie/yaml-front-matter": "^2.0",
        "symfony/yaml": "^3.0|^4.0|^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "RTNatePHP\\LaravelFileContentUpdates\\ServiceProvider"
            ],
            "aliases": {
                "ContentUpdates": "RTNatePHP\\LaravelFileContentUpdates\\Facades\\ContentUpdates"
            }
        }
    }
}
